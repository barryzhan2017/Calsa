; ModuleID = 'MicroC'
source_filename = "MicroC"

@fmt = private unnamed_addr constant [4 x i8] c"%d\0A\00", align 1
@0 = private unnamed_addr constant [2 x i8] c"3\00", align 1

declare i32 @printf(i8*, ...)

define i32 @main() {
entry:
  %x = alloca [5 x i32]
  %y = alloca i8*
  store i8* getelementptr inbounds ([2 x i8], [2 x i8]* @0, i32 0, i32 0), i8** %y
  %y1 = load i8*, i8** %y
  %printf = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @fmt, i32 0, i32 0), i8* %y1)
  store [5 x i32] [i32 1, i32 2, i32 3, i32 4, i32 5], [5 x i32]* %x
  ret i32 0
}
