List map(List l, func f){
    int len = l.size();
    int i = 0;
    while (i < len){
        int tmp = f(l.get(i));
        l.set(i, tmp);
        i = i + 1;
    }
    return l;
}

int foldleft(List l, int acc, func f) {
    int len = l.size();
    int i = 0;
    while (i < len){
        acc = f(acc, l.get(i));
        i = i + 1;
    }
    return acc;
}

int main() {
    List a;
    a.add(1);
    a.add(2);
    a.add(3);
    func f = int b(int x) {
        return x + 10;
    };
    print(a);
    List b = a.map(f);
    print(b);
}
